mixin carousel(items)
  .youplay-carousel(data-autoplay='5000')
    each item in items
      a.angled-img(href=item.url)
        .img
          img(src='/images/cats/500/#{item._id}.jpg', alt='')
        .over-info
          div
            div
              h4= item.name
              h5= item.subtitle
              p.btn.btn-xs(href='#!') بازی نامه
